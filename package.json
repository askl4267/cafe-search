  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.12.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.47.0"
  }
}
